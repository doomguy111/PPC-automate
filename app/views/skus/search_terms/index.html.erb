<%= breadcrumbs_helper([
  { text: 'Dashboard', path: root_path },
  { text: 'SKUs', path: skus_path },
  { text: @sku.name, path: sku_path(@sku) },
  { text: 'Search Terms', path: '' }
]) %>

<table class="table table-bordered dataTable" id="dataTable" width="100%" cellspacing="0" role="grid" aria-describedby="dataTable_info" style="width: 100%;">
  <thead>
    <tr>
      <th>Term</th>
      <th>Reported Days</th>
      <th>Impressions</th>
      <th>Clicks</th>
      <th>CTR</th>
      <th>Spend</th>
      <th>Sales</th>
      <th>ACoS</th>
    </tr>
  </thead>
  <tbody>
    <%= render @search_terms %>
  </tbody>
</table>
